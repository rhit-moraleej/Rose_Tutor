<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RoseTutorTracker" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaryColor</item>
<!--        <item name="colorPrimaryVariant">@color/color</item>-->
<!--        <item name="colorOnPrimary">@color/colo</item>-->

        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="android:textColorPrimary">@color/primaryTextColor</item>

        <!-- Secondary brand color. -->
<!--        <item name="colorSecondary">@color/colo</item>-->
<!--        <item name="colorSecondaryVariant">@color/teal_700</item>-->
<!--        <item name="colorOnSecondary">@color/black</item>-->
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">@font/poppins</item>
        <item name="font">@font/poppins</item>
    </style>

    <style name="Theme.RoseTutorTracker.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.RoseTutorTracker.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RoseTutorTracker.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="tutorDetails">
        <item name="android:textSize">20sp</item>
    </style>
</resources>